    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-paginate": "^6.5.0",
    "rxjs": "^6.6.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "@typescript-eslint/parser": "^4.8.2",
    "eslint": "^7.14.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "prettier": "2.1.2",
    "typescript": "^4.1.2"
